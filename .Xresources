URxvt.internalBorder: 10

URxvt.font: xft:Dejavu Sans Mono:size=14,xft:FiraCode Nerd Font Mono:style=Retina,Regular
!URxvt.font: xft:Symbols Nerd Font Mono:size=14
!,xft:devicons:size=14,xft:Dejavu Sans Mono:size=14
!URxvt.font: xft:Dejavu Sans Mono:size=14,xft:Symbols Nerd Font Mono
!URxvt.font: xft:Dejavu Sans Mono:size=14,xft:Symbols Nerd Font Mono:size=14,xft:devicons:size=14
